{
	"name": "vscode-elementui-scss-plugin",
	"displayName": "vscode-elementui-scss-plugin",
	"description": "自动提示补全scss变量",
	"version": "0.0.1",
	"publisher": "afsfe-zhuhai",
	"repository": {
		"type": "git",
		"url": "https://github.com/openjavascript/vscode-elementui-scss-plugin.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:vue",
		"onLanguage:javascript",
		"onLanguage:scss"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"sync:var": "yarn && cp -f ./node_modules/@qax/qax-ui/lib/scss-variable.* ./src/assets && node ./script/sync-variables.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@qax/qax-ui": "^2.13.0-42",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
